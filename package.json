{
  "name": "cueless",
  "version": "0.1.0",
  "private": true,
  "description": "IM-first control layer for dispatching agent runtimes",
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "start": "pnpm --filter @cueless/core start",
    "test": "turbo run test",
    "test:e2e": "pnpm --filter @cueless/core test:e2e",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "^2.5.8",
    "typescript": "^5.7.3"
  }
}